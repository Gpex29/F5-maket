div.list
  each job in jobListData
    div.list-item
      div.list-item-header
        div.icon-layout 
          if job.icon
            picture
              img.icon(src=`img/section1/${job.icon}`, alt=job.company)
        div.list-item-header-title
          div= job.company
          div.inter.fs14.fw400.text-gray= job.date
      h6.list-item-h6.archivo.fs18.fw700= job.title
      div.list-item-job-tags
        div.job-category= job.category
        div.job-tags= job.type
      div.list-item-job-details
        picture
            img.list-item-job-details-img(src="./img/section1/location.png", alt="location")
        p.list-item-job-details-text= job.location
        picture
            img.list-item-job-details-img(src="./img/section1/money.png", alt="money")
        p.list-item-job-details-text= job.salary